#include <stdio.h>
void main(){
    float pi=3.14;
    float radius;
    float height;
    printf("enter a radius: ");
    scanf("%f",&radius);
    printf("enter a height: ");
    scanf("%f",&height);
    printf("volume of cylinder:%f",pi*radius*radius*height);
}
*********************************************************************
OUTPUT
enter a radius: 5.8
enter a height: 3.9
volume of cylinder:411.955505

